pkgname=lolix-settings
pkgver=1
pkgrel=6
pkgdesc="lolix stuff"
arch=('any')
url="https://github.com/FabioLolix/PKGBUILD"
license=('none')
depends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
source=(customav.list customav2.list customav3.list)
md5sums=('SKIP' 'SKIP' 'SKIP')
package() {
  install -Dm644 customav.list ${pkgdir}/etc/manjaro-tools/pkg.list.d/customav.list
  install -Dm644 customav2.list ${pkgdir}/etc/manjaro-tools/pkg.list.d/customav2.list
  install -Dm644 customav3.list ${pkgdir}/etc/manjaro-tools/pkg.list.d/customav3.list
}
